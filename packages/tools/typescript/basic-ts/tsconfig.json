{
    "compilerOptions": {
      "declaration": false,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "lib": [
        "es2015",
        "es2016",
        "es2017",
        "es2018",
        "esnext",
        "dom"
      ],
      "module": "commonjs",
      "moduleResolution": "node",
      "noImplicitThis": false,
      "noImplicitReturns": true,
      "outDir": "dist",
      "sourceMap": false,
      "strict": true,
      "target": "es2015"
    },
    "exclude": [
      "node_modules",
      "dist"
    ]
  }